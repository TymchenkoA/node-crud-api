  "bugs": {
    "url": "https://github.com/TymchenkoA/node-crud-api/issues"
  },
  "homepage": "https://github.com/TymchenkoA/node-crud-api#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.25.8",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
